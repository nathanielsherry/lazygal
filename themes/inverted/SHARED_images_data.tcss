/* ============================================================================
Free icons from http://www.axialis.com/free/icons/index.html
Icon set 'Web 2.0 Buttons - Rounded'
========================================================================== */

.image_size ul li:first-child a,
.image_size ul li:first-child span {
    /* Reduced Size Blue.png */
    background-image: url('data:image/png;base64,');
    
    background-attachment: center;
    background-color: transparent;
    background-position: left;
    background-repeat: no-repeat;
    padding-left: 18px;
    padding-bottom:1px;
}

.image_size ul li:last-child a,
.image_size ul li:last-child span {
    /* Full Size Blue.png */
    background-image: url('data:image/png;base64,');
    
    background-attachment: center;
    background-color: transparent;
    background-position: right;
    background-repeat: no-repeat;
    padding-right: 18px;
    padding-bottom:1px;
}

